<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Matchimizer.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.js")" type="text/javascript"></script>
     <script src="@Url.Content("~/Scripts/matchimizer.js")" type="text/javascript"></script>


</head>
<body>
    <div class="page">
        <div id="header">
            <div id="title">
                <h1>Customizer Matching</h1>
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <div id="menucontainer">
                <ul id="menu">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Upload Xml","Index", "ImportXml")</li>
                    <li>@Html.ActionLink("Update Tool","Index", "UpdateNutrionalInformation")</li>
                    <li>@Html.ActionLink("Matched Items","MatchedMenuItems", "MatchedItems")</li>
                    <li>@Html.ActionLink("Partial Matches", "PartialMatchMenuItems", "MatchedItems")</li>
                    <li>@Html.ActionLink("Un-matched Items","UnmatchedMenuItems", "MatchedItems")</li>
                    <li>@Html.ActionLink("Manage Duplicates","Index", "ManageDuplicates")</li>
                </ul>
            </div>
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
        </div>
    </div>
</body>
</html>
